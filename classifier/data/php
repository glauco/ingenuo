<?php//load the wsdl file$client = new SoapClient("http://example.com/soap/definition.wsdl");//functions are now available to be called$result = $client->soapFunc("hello");$result = $client->anotherSoapFunc(34234); //SOAP Information$client = new SoapClient("http://example.com/soap/definition.wsdl");//list of SOAP typesprint_r($client->__getTypes());//list if SOAP Functionsprint_r($client->__getFunctions());?>